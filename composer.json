{
    "name": "arachne/service-collections",
    "description": "Resolvers and generators of services for nette/di. Used by arachne/forms and other libraries.",
    "type": "library",
    "keywords": [ "nette", "arachne", "di" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jáchym Toušek",
            "homepage": "http://enumag.cz",
            "email": "enumag@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "nette/di": "^2.4.0",
        "nette/utils": "^2.4.0"
    },
    "require-dev": {
        "arachne/codeception": "^0.7.3",
        "codeception/codeception": "^2.2.0",
        "eloquent/phony": "^0.14.0"
    },
    "autoload": {
        "psr-4": {
            "Arachne\\ServiceCollections\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/unit/src",
            "Tests\\Integration\\": "tests/integration/src"
        }
    }
}
